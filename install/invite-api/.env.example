# App Store Connect API認証情報
ASC_ISSUER_ID=your-issuer-id
ASC_KEY_ID=your-key-id
ASC_PRIVATE_KEY=your-private-key-path
ASC_BUNDLE_ID=com.atomspring

# TestFlight設定
BETA_GROUP_ID=your-beta-group-id
BETA_APP_ID=your-app-id

# サーバー設定
PORT=3000
NODE_ENV=development

# メール設定
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=your-smtp-user
SMTP_PASS=your-smtp-password
MAIL_FROM=noreply@atomspring.example.com

# 管理者設定
ADMIN_EMAIL=admin@atomspring.example.com
SUPPORT_EMAIL=support@atomspring.example.com

# セキュリティ設定
JWT_SECRET=your-jwt-secret
RATE_LIMIT_WINDOW_MS=900000  # 15分
RATE_LIMIT_MAX_REQUESTS=100